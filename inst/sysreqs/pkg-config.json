{
  "patterns": ["\\bpkg-config\\b", "\\bpkgconfig\\b"],
  "dependencies": [
    {
      "packages": ["pkg-config"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "ubuntu"
        },
        {
          "os": "linux",
          "distribution": "debian"
        }
      ]
    },
    {
      "packages": ["pkgconfig"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "centos",
          "versions": ["6", "7"]
        },
        {
          "os": "linux",
          "distribution": "redhat",
          "versions": ["6", "7"]
        }
      ]
    },
    {
      "packages": ["pkgconf-pkg-config"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "centos",
          "versions": ["8"]
        },
        {
          "os": "linux",
          "distribution": "rockylinux"
        },
        {
          "os": "linux",
          "distribution": "redhat",
          "versions": ["8", "9"]
        },
        {
          "os": "linux",
          "distribution": "fedora"
        }
      ]
    },
    {
      "packages": ["pkg-config"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "opensuse"
        },
        {
          "os": "linux",
          "distribution": "sle"
        }
      ]
    },
    {
      "packages": ["pkgconf"],
      "constraints": [
        {
          "os": "linux",
          "distribution": "alpine"
        }
      ]
    }
  ]
}
